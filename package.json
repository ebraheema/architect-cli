{
  "name": "@architect-io/cli",
  "description": "Command line interface for creating and deploying architect services",
  "version": "0.1.8",
  "author": "David Thor @davidthor",
  "bin": {
    "architect": "./bin/run"
  },
  "bugs": "https://github.com/architect-team/architect-cli/issues",
  "dependencies": {
    "@architect-io/node-launcher": "^0.1.2",
    "@architect-io/python-launcher": "^0.1.11",
    "@oclif/command": "^1.5.2",
    "@oclif/config": "^1.8.6",
    "@oclif/plugin-help": "^2.1.2",
    "chalk": "^2.4.1",
    "inquirer": "^6.2.0",
    "oclif": "^1.12.5",
    "protocol-buffers": "^4.1.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.0",
    "@oclif/test": "^1.2.2",
    "@oclif/tslint": "^3.1.0",
    "@types/chai": "^4.1.6",
    "@types/inquirer": "0.0.43",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.5",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "sinon": "^6.3.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/architect-team/architect-cli",
  "keywords": [
    "oclif"
  ],
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "architect",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "architect-team/architect-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postinstall": "./bin/postinstall.sh",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
